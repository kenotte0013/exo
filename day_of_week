#include <stdio.h>
#include <stdlib.h>

int day;
char week [][9] = {"Lundi", "Mardi", "Mercredi", "Jeudi", "Vendredi", "Samedi", "Dimanche"};
int main()
{

printf("jour de la semaine\n");
scanf("%d", &day);
switch (day)
    {
        case 1 :
            printf("%s\n", week[0]);
            break;
        case 2 :
            printf("%s\n", week[1]);
            break;
        case 3 :
            printf("%s\n", week[2]);
            break;
        case 4 :
            printf("%s\n", week[3]);
            break;
        case 5 :
            printf("%s\n", week[4]);
            break;
        case 6 :
            printf("%s\n", week[5]);
            break;
        case 7 :
            printf("%s\n", week[6]);
            break;
    }
    
    return 0;
}
